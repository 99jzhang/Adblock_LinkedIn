{
    "manifest_version": 3,
    "name": "LinkedIn AdBlocker",
    "description": "Blocking ads on LinkedIn Desktop Broswer.",
    "version": "1.0.0",
    "author": "Jonathan Zhang",
    "permissions": [
        "scripting",
        "activeTab",
        "tabs",
        "webNavigation"
    ],
    "host_permissions": [
        "https://www.linkedin.com/",
        "<all_urls>",
        "*://*/*"
    ]
}