{
    "manifest_version": 2,
    "name": "LinkedIn AdBlocker",
    "description": "Blocking ads on LinkedIn Desktop Broswer.",
    "version": "0.0.1",
    "author": "Jonathan Zhang",
    "browser_action": {
        "default_title": "LinkedIn AdBlocker"
    },
    "permissions": [
        "tabs",
        "webNavigation",
        "https://www.linkedin.com/"
    ],
    "background": {
        "scripts": [
            "extension.js"
        ]
    }
}